class Solution {
    public int minOperations(int[] nums) {
  int n=nums.length;
int ones=0;
for(int x:nums){
if(x==1)ones++;
}
if(ones>0)return n-ones;
int minops=Integer.MAX_VALUE;
for(int i=0;i<n;i++){
int g=nums[i];
for(int j=i+1;j<n;j++){
g=gcd(g,nums[j]);
if(g==1){
minops=Math.min(minops,j-i);
break;
}
}
}
 if(minops==Integer.MAX_VALUE){
        return -1;
       }
       return minops+n-1;
    }
       public int gcd(int a,int b){
        return b==0?a:gcd(b,a%b);
       }
    }
